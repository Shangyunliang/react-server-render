{
  "name": "react-server-render",
  "version": "1.0.0",
  "description": "react server render",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shangyunliang/react-server-render.git"
  },
  "keywords": [
    "react",
    "server-render"
  ],
  "author": "SYL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Shangyunliang/react-server-render/issues"
  },
  "homepage": "https://github.com/Shangyunliang/react-server-render#readme",
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "^4.0.0-beta.13",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  }
}
